<div id="container" style="width:100%; height:400px;">

  <script>
    $(function () {
      $('#container').highcharts({
          title: {
              text: 'Weekly Mastercoin Transactions',
              x: -20 //center
          },
          subtitle: {
              text: 'MSC excluding test coins',
              x: -20
          },
          xAxis: {
              categories: <%= raw SimpleSend.collect_date.to_json %>
          },
          yAxis: {
              title: {
                  text: 'Amount transferred'
              },
              plotLines: [{
                  value: 0,
                  width: 1,
                  color: '#808080'
              }]
          },
          tooltip: {
              valueSuffix: 'MSC'
          },
          legend: {
              layout: 'vertical',
              align: 'right',
              verticalAlign: 'middle',
              borderWidth: 0
          },
          series: [ <%= raw SimpleSend.collect_chart_data.to_json %> ]
	    });
		});
  </script>
</div>
